#carousel-sponsors(class="carousel carousel-light slide" data-ride="carousel" data-interval=4000)
  %ol.carousel-indicators
    %li(data-target="#carousel-sponsors" data-slide-to="0" class="active")
    - 1.upto(3) do |i|
      %li(data-target="#carousel-sponsors" data-slide-to="#{i}" )
  .carousel-inner
    .item.active
      %img.img-responsive(src="/attachments/sponsors/koni.png" title="Koni" alt="Koni")
      .carousel-caption
        %h2.text-primary Koni
    .item
      %img.img-responsive(src="/attachments/sponsors/s711.png" title="7-11 stores" alt="7-11")
      .carousel-caption
        %h2.text-primary 7-11 Stores in Jakarta
    .item
      %img.img-responsive(src="/attachments/sponsors/xl.png" title="xl" alt="xl")
      .carousel-caption
        %h2.text-primary xl
    .item
      %img.img-responsive(src="/attachments/sponsors/tcel.png" title="telkomsel" alt="telkomsel")
      .carousel-caption
        %h2.text-primary Telkomsel
    .item
      %img.img-responsive(src="/attachments/sponsors/eagle.png" title="telkomsel" alt="telkomsel")
      .carousel-caption
        %h2.text-primary Eagle
  %a(class="left carousel-control" href="#carousel-sponsors" data-slide="prev" role="button")
    %span(class="icon icon-chevron-thin-left" aria-hidden="true")
    %span(class="sr-only")
      Previous
  %a(class="right carousel-control" href="#carousel-sponsors" data-slide="next" role="button")
    %span(class="icon icon-chevron-thin-right" aria-hidden="true")
    %span(class="sr-only")
      Next